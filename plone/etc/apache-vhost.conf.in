<Directory c:\Project\freia\plone/parts/wsgiapp>
    Order deny,allow
    Allow from all
</Directory>

<VirtualHost *:80>
    ServerName www.freia.jp
    #ServerAlias admin.www.freia.jp zmi.www.freia.jp
    CustomLog /var/log/httpd/www.freia.jp-access.log combined
    ErrorLog /var/log/httpd/www.freia.jp-error.log

    WSGIDaemonProcess www.freia.jp processes=2 threads=2 maximum-requests=10000 user=www group=www
    WSGIScriptAlias / c:\Project\freia\plone/parts/wsgiapp/wsgi
    WSGIProcessGroup www.freia.jp
    WSGIPassAuthorization On

    #SetEnvIf Host "(.*)" HTTP_X_VHM_HOST=http://$1
    #SetEnvIf Host "^(?!zmi.*).*$" HTTP_X_VHM_ROOT=/{apache-conf:plonesite-id}
    #SetEnv DISABLE_PTS 1
</VirtualHost>

